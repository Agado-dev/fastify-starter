{
  "name": "@agado/api-starter",
  "version": "1.0.0",
  "type": "module",
  "private": true,
  "engines": {
    "node": "22.x.x"
  },
  "author": "Kevin Pennarun",
  "scripts": {
    "dev": "npm-run-all --parallel compile:watch tsc:watch serve:debug",
    "build": "prisma generate && pnpm compile && pnpm tsc && pnpm lint",
    "start": "node --import @sentry/node/preload ./dist/main.js",
    "compile": "swc src/ --out-dir dist/ --copy-files --strip-leading-paths",
    "compile:watch": "pnpm compile -w --source-maps",
    "tsc:watch": "tsc --emitDeclarationOnly --watch",
    "serve:debug": "nodemon --inspect --trace-warnings --import @sentry/node/preload dist/main.js",
    "prisma:generate": "prisma generate",
    "prisma:migrate": "prisma migrate deploy && prisma db seed",
    "test:unit": "CONFIG_PATH=.env.test vitest",
    "test": "npm-run-all --parallel test:unit test:integration",
    "test:integration": "CONFIG_PATH=.env.test vitest -c vitest.integration.config.ts",
    "format": "biome format . --write",
    "lint": "biome lint .",
    "lint:fix": "biome lint . --write",
    "check": "biome check ."
  },
  "dependencies": {
    "@agado/api-starter-contract": "workspace:*",
    "@fastify/awilix": "^7.0.0",
    "@fastify/error": "^4.0.0",
    "@fastify/multipart": "^9.0.3",
    "@fastify/request-context": "^6.0.2",
    "@fastify/swagger": "^9.4.2",
    "@fastify/swagger-ui": "^5.2.0",
    "@fastify/under-pressure": "^9.0.2",
    "@paralleldrive/cuid2": "^2.2.2",
    "@prisma/client": "^6.3.1",
    "@prisma/extension-optimize": "^1.1.4",
    "@sentry/node": "^8.54.0",
    "@sentry/profiling-node": "^8.54.0",
    "@sentry/utils": "^8.54.0",
    "@ts-rest/core": "^3.51.1",
    "@ts-rest/fastify": "^3.51.1",
    "@ts-rest/open-api": "^3.52.0",
    "app-root-path": "^3.1.0",
    "auth0": "^4.18.0",
    "awilix": "^12.0.4",
    "convict": "^6.2.4",
    "convict-format-with-validator": "^6.2.0",
    "dotenv": "^16.4.7",
    "dotenv-expand": "^12.0.0",
    "fastify": "^5.2.1",
    "fastify-plugin": "^5.0.1",
    "jsonwebtoken": "^9.0.2",
    "jwks-rsa": "^3.1.0",
    "pino": "^9.6.0",
    "pino-pretty": "^13.0.0",
    "prisma": "^6.3.1",
    "result-in-ts": "^1.0.0",
    "source-map-support": "^0.5.21",
    "zod": "^3.24.1"
  },
  "devDependencies": {
    "@biomejs/biome": "^1.9.4",
    "@swc/cli": "^0.6.0",
    "@swc/core": "^1.10.15",
    "@testcontainers/postgresql": "^10.18.0",
    "@total-typescript/ts-reset": "^0.6.1",
    "@types/convict": "^6.1.6",
    "@types/convict-format-with-validator": "^6.0.5",
    "@types/jsonwebtoken": "^9.0.8",
    "@types/node": "^22.13.1",
    "@types/source-map-support": "^0.5.10",
    "nodemon": "^3.1.9",
    "npm-run-all": "^4.1.5",
    "testcontainers": "^10.18.0",
    "tsx": "^4.19.2",
    "typescript": "^5.7.3",
    "vitest": "^3.0.6"
  },
  "prisma": {
    "seed": "tsx prisma/seed.ts"
  }
}
